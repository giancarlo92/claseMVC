@model IEnumerable<InterfacesInventario.Productos.Respuestas.ProductoRegistrado>

@{ 
    Layout = null;
}

<br />
<table class="table">
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(model => model.Id)</th>
            <th>@Html.DisplayNameFor(model => model.Nombre)</th>
            <th>@Html.DisplayNameFor(model => model.Precio)</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Nombre</td>
                <td>@item.Precio</td>
            </tr>
        }
    </tbody>
</table>
